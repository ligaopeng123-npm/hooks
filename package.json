{
    "version": "0.1.7",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "dev": "set NODE_ENV=development&&webpack serve",
        "docs": "set NODE_ENV=production&&webpack build",
        "build": "lerna exec --ignore=@gaopeng123/hooks -- npm run build",
        "test": "lerna exec --ignore=@gaopeng123/hooks -- npm run test",
        "push": "lerna publish",
        "pushAll": "lerna exec -- npm run push",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "peerDependencies": {
        "react": ">=16.8"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "name": "@gaopeng123/hooks",
    "author": "ligaopeng123",
    "repository": {
        "type": "git",
        "url": "https://github.com/ligaopeng123-npm/hooks.git"
    },
    "module": "dist/hooks.esm.js",
    "size-limit": [
        {
            "path": "dist/hooks.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/hooks.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^5.0.1",
        "@types/react": "^17.0.15",
        "@types/react-dom": "^17.0.9",
        "@types/react-router-dom": "^5.3.2",
        "babel-loader": "^8.2.3",
        "html-webpack-plugin": "^5.3.2",
        "husky": "^7.0.1",
        "lerna": "^4.0.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^5.3.0",
        "size-limit": "^5.0.1",
        "ts-loader": "^9.2.5",
        "tsdx": "^0.14.1",
        "tslib": "^2.3.0",
        "typescript": "^4.3.5",
        "webpack": "^5.51.1",
        "webpack-cli": "^4.8.0",
        "webpack-dev-server": "^4.0.0"
    }
}
