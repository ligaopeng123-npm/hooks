{
    "version": "0.1.1",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "private": false,
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "tsdx test --passWithNoTests",
        "push": "npm publish --access public",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "peerDependencies": {
        "react": ">=16"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "name": "@gaopeng123/hooks",
    "author": "ligaopeng123",
    "repository": {
        "type": "git",
        "url": "https://github.com/ligaopeng123-npm/hooks.git"
    },
    "module": "dist/hooks.esm.js",
    "size-limit": [
        {
            "path": "dist/hooks.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/hooks.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^5.0.1",
        "@types/react": "^17.0.15",
        "@types/react-dom": "^17.0.9",
        "husky": "^7.0.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "size-limit": "^5.0.1",
        "tsdx": "^0.14.1",
        "tslib": "^2.3.0",
        "typescript": "^4.3.5"
    },
    "dependencies": {
        "@gaopeng123/utils": "^0.1.22"
    }
}
